function [lp] = maxl(i,j,k)
global Q
global n
global m

if (k >= 1) && (k < Q)
    if (j == 1)
        lp = 4;
    elseif (j > 1) && (j < n)
        lp = 5;
    elseif (j == n)
        lp = 3;
    end
end
    for i = i:m

        if (j >= 1) && (j < n)
            lp = 2
        end
        if (j == n)
            lp = 1
        end
    end
end

if (k == Q)
    if (i == m) && (j == n) && (k == Q)
        if ( i <= m-1)
            if (j == 1)
                lp = 3
            elseif (j > 1) && (j < n)
                lp = 4
            else (j == n)
                lp = 2
            end
            end
            end
        end
    end
end
end